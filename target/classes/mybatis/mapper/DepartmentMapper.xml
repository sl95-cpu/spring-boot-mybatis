<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0/EN "
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sl.srpingboot001.mapper.DepartmentMapper">
    <select id="getDepartment" resultType="department">
        select * from persons.department;
    </select>
    <select id="getDepartmentById" resultType="department" parameterType="int">
        select * from persons.department where id=#{id}
    </select>
</mapper>

